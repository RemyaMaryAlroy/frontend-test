<!doctype html>
<html class="no-js" lang="de">
<head>
  <meta charset="utf-8">
  <title>Frontend Test</title>
  <meta name="description" content="">
  <meta http-equiv="x-ua-compatible" content="ie=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="css/vendor.min.css">
  <link rel="stylesheet" href="css/main.min.css">

</head>
<body>
  <div class="container sitewrapper">

    <% headerData  = JSON.parse(headerData) %>
    <% contentData = JSON.parse(contentData) %>
    <% footerData  = JSON.parse(footerData) %>

    <!-- HEADER -->

    <% if (headerData) { %>
      <!-- header/header -->
      <%- include( headerData.templatePath, headerData ) %>
    <% } %>

    <!-- CONTENT -->
    <% if (contentData) { %>
      <!-- content/content -->
      <%- include( contentData.templatePath, contentData ) %>
    <% } %>

    <!-- FOOTER -->
    <%# if (footerData) { %>
      <!-- footer/footer -->
      <%- include( footerData.templatePath, footerData ) %>
    <%# } %>

  </div>

  <script src="js/vendor.min.js"></script>
  <script src="js/main.js"></script>
</body>
</html>
